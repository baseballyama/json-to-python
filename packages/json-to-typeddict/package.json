{
  "name": "json-to-typeddict",
  "description": "Simple utility to convert JSON to Python TypedDict",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./src/cli.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./src/index.mjs"
    }
  },
  "scripts": {
    "format:fix": "prettier --cache --write \"./src/**/*.{js,ts,json}\"",
    "format:check": "prettier --cache --check \"./src/**/*.{js,ts,json}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "publish:check": "publint",
    "test:all": "pnpm format:check && pnpm test && pnpm type:gen && pnpm publish:check",
    "type:gen": "rm -rf dist && tsc -P tsconfig.json --emitDeclarationOnly"
  },
  "dependencies": {
    "publint": "^0.2.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.33.0"
  }
}
